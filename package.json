{
  "name": "portfolio-website",
  "version": "1.0.0",
  "description": "Confidential MERN Stack Portfolio with AI Assistant",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend:dev\" \"npm run frontend:dev\" \"npm run ai:dev\"",
    "backend:dev": "cd backend && npm run dev",
    "frontend:dev": "cd frontend && npm run dev",
    "ai:dev": "cd ai-service && python simple_app.py",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": ["portfolio", "mern", "ai", "confidential"],
  "author": "Your Name",
  "license": "PRIVATE",
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
